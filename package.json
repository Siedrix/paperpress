{
  "name": "paperpress",
  "version": "1.0.0",
  "main": "./paperpress",
  "description": "A static pages generator for Node.js.",
  "scripts": {
    "test": "./node_modules/mocha/bin/_mocha -R spec && npm run lint",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R nyan",
    "report": "npm run coverage && open coverage/lcov-report/paperpress/paperpress.js.html",
    "lint": "eslint gulpfile.js paperpress.js test/test.js; true"
  },
  "keywords": [
    "express",
    "markdown",
    "publishing"
  ],
  "license": "MIT",
  "dependencies": {
    "feed": "~0.3.0",
    "highlight.js": "^9.3.0",
    "remarkable": "^1.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Siedrix/paperpress"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "eslint": "^2.8.0",
    "eslint-config-standard": "^5.2.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-mocha": "^2.2.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "underscore": "^1.8.3"
  }
}
